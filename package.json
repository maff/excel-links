{
  "name": "yet-another-webpack-es6-starterkit",
  "version": "1.0.0",
  "description": "A light foundation for your next frontend project based on webpack.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_ENV=development npm-run-all --parallel wp-dev start",
    "wp-build": "webpack --config webpack-prod.config.js --watch --colors",
    "wp-dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/yet-another-webpack-es6-starterkit.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbkd/yet-another-webpack-es6-starterkit/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "homepage": "https://github.com/wbkd/yet-another-webpack-es6-starterkit#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.17.0",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.9.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.6",
    "dropzone": "^4.3.0",
    "ejs": "^2.5.5",
    "exceljs": "^0.2.38",
    "express": "^4.14.0",
    "file-saver": "^1.3.3",
    "jquery": "^3.1.1",
    "npm-run-all": "^4.0.0"
  }
}
